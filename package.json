{
  "name": "porter-logistics-platform",
  "version": "1.0.0",
  "description": "Complete logistics and transport booking platform like Porter",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "build": "tsc && pnpm --filter admin build",
    "build:backend": "tsc",
    "build:admin": "pnpm --filter admin build",
    "install:all": "pnpm install && pnpm --filter driver-app install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "logistics",
    "transport",
    "booking",
    "porter",
    "react-native",
    "nodejs",
    "typescript"
  ],
  "author": "Porter Platform Team",
  "license": "MIT",
  "dependencies": {
    "@redis/client": "^5.10.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^10.4.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "razorpay": "^2.9.2",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.7.0",
    "twilio": "^4.19.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
