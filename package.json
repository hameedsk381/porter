    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.22",
    "@types/redis": "^4.0.10",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
